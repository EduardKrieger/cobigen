<?xml version="1.0" encoding="UTF-8"?>
<tns:templateSetConfiguration version="6.0" xmlns:tns="http://capgemini.com/devonfw/cobigen/TemplateSetConfiguration" xmlns:tns1="http://capgemini.com/devonfw/cobigen/ContextConfiguration" xmlns:tns2="http://capgemini.com/devonfw/cobigen/TemplatesConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://capgemini.com/devonfw/cobigen/TemplateSetConfiguration templateSetConfiguration.xsd ">
  <tns1:contextConfiguration version="3.0">
      <tns1:trigger id="crud_openapi_angular_service_based_app" type="openapi" templateFolder="crud_openapi_angular_service_based_app">
    <tns1:containerMatcher type="element" value="openAPIFile"/>
    <tns1:matcher type="element" value="ComponentDef">
      <tns1:variableAssignment type="constant" key="domain" value="demo"/>
      <tns1:variableAssignment type="property" key="component" value="name"/>
      <tns1:variableAssignment type="property" key="etoName" value="name"/>
    </tns1:matcher>
  </tns1:trigger>
    <tns1:tags>
      <tns1:tag name="bla" />
    </tns1:tags>
  </tns1:contextConfiguration>
  <tns2:templatesConfiguration version="5.0">
<tns2:templates>
    <tns2:templateExtension ref="environment.ts" mergeStrategy="tsmerge_override"/>
  </tns2:templates>

  <tns2:templateScans>
    <tns2:templateScan name="ts_scan" templatePath="templates" destinationPath=""/>
  </tns2:templateScans>

  <tns2:increments>
    <tns2:increment name="app_angular_env" description="Server URL Registration">
      <tns2:templateRef ref="environment.ts"/>
    </tns2:increment>
  </tns2:increments>
  </tns2:templatesConfiguration>
</tns:templateSetConfiguration>
